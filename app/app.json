{
  "expo": {
    "name": "Echoify",
    "slug": "Echoify",
    "version": "0.2.0-alpha",
    "orientation": "portrait",
    "icon": "./assets/ic_launcher.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/ic_launcher.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/ic_launcher_foreground.png",
        "backgroundImage": "./assets/ic_launcher_background.png",
        "monochromeImage": "./assets/ic_launcher_monochrome.png"
      },
      "splash": {
        "dark": {
          "backgroundColor": "#232023"
        }
      },

      "edgeToEdgeEnabled": true,
      "package": "io.github.ronitkrshah.echoify"
    },
    "plugins": [
      [
        "react-native-bottom-tabs",
        {
          "theme": "material3-dynamic"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/ic_launcher.png",
          "dark": {
            "backgroundColor": "#202320"
          },
          "imageWidth": 200
        }
      ],
      "expo-sqlite"
    ]
  }
}
